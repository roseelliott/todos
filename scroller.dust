<div id="entry">
  <input id="new-todo" type="text" placeholder="What needs to be done?">
</div>

<div id="wrapper">

  <div id="scroller">
    <div id="pullDown">
      <span class="pullDownIcon"></span>
      <span class="pullDownLabel"></span>
    </div>

    <div id="main">
      <input id="toggle-all" type="checkbox">
      <label for="toggle-all">Mark all as completed</label>
      <ul id="todo-list"></ul>
    </div>

  </div>

</div>

